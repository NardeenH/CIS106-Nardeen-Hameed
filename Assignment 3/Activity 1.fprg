    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="Create a program to prompt the user for hours and rate per hour and then calculate and display their weekly, monthly, and annual gross pay (hours * rate). Base monthly and annual calculations on 12 months per year and 52 weeks per year.&#13;&#10;&#13;&#10;&#13;&#10;"/>
            <declare name="hours" type="Real" array="False" size=""/>
            <declare name="rate" type="Real" array="False" size=""/>
            <declare name="weekly" type="Real" array="False" size=""/>
            <declare name="daily" type="Real" array="False" size=""/>
            <declare name="yearly" type="Real" array="False" size=""/>
            <declare name="monthly" type="Real" array="False" size=""/>
            <output expression="&quot; How many weekly hours did you work ?&quot;" newline="True"/>
            <input variable="Hours"/>
            <output expression="&quot; What is your rate foy pay ?&quot;" newline="True"/>
            <input variable="Rate"/>
            <assign variable="Weekly" expression="rate*hours"/>
            <assign variable="monthly" expression="weekly*30"/>
            <assign variable="yearly" expression="weekly*30*12"/>
            <output expression="&quot;Salary in Weekly: &quot; &amp; weekly" newline="True"/>
            <output expression="&quot;Salary in Monthly: &quot; &amp; monthly" newline="True"/>
            <output expression="&quot;Salary in Yearly: &quot; &amp; yearly" newline="True"/>
        </body>
    </function>
