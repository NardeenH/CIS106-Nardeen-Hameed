    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="Create a program to prompt the user for hours and rate per hour and then calculate and display their weekly, monthly, and annual gross pay (hours * rate). Base monthly and annual calculations on 12 months per year and 52 weeks per year.[1]&#13;&#10;I got some help from Tim.&#13;&#10;&#13;&#10;"/>
            <declare name="hours" type="Integer" array="False" size=""/>
            <declare name="rate" type="Integer" array="False" size=""/>
            <declare name="daily" type="Integer" array="False" size=""/>
            <declare name="weekly" type="Integer" array="False" size=""/>
            <declare name="monthly" type="Integer" array="False" size=""/>
            <declare name="yearly" type="Integer" array="False" size=""/>
            <output expression="&quot; How many weekly hours did you work ?&quot;" newline="True"/>
            <input variable="Hours"/>
            <output expression="&quot; What is your rate foy pay ?&quot;" newline="True"/>
            <input variable="Rate"/>
            <assign variable="daily" expression="hours*rate"/>
            <assign variable="weekly" expression="daily*7"/>
            <assign variable="monthly" expression="daily*30"/>
            <assign variable="yearly" expression="weekly*52"/>
            <output expression="&quot;Salary in Weekly: &quot; &amp; weekly" newline="True"/>
            <output expression="&quot;Salary in Monthly: &quot; &amp; monthly" newline="True"/>
            <output expression="&quot;Salary in Yearly: &quot; &amp; yearly" newline="True"/>
        </body>
    </function>
