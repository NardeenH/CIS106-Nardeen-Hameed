    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="age" type="Integer" array="False" size=""/>
            <declare name="month" type="Integer" array="False" size=""/>
            <declare name="day" type="Integer" array="False" size=""/>
            <output expression="&quot;age&quot;" newline="True"/>
            <input variable="age"/>
            <output expression="&quot; Month &quot;" newline="True"/>
            <input variable="month"/>
            <output expression="&quot; Day &quot;" newline="True"/>
