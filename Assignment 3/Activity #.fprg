    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="Create a program to prompt the user for hours and rate per hour and then calculate and display their weekly, monthly, and annual gross pay (hours * rate). Base monthly and annual calculations on 12 months per year and 52 weeks per year.[1]"/>
            <declare name="hours" type="Integer" array="False" size=""/>
            <declare name="rate" type="Integer" array="False" size=""/>
            <declare name="daily" type="Integer" array="False" size=""/>
            <declare name="weekly" type="Integer" array="False" size=""/>
            <declare name="monthly" type="Integer" array="False" size=""/>
            <declare name="annualy" type="Integer" array="False" size=""/>
            <input variable="Hours"/>
            <input variable="Rate"/>
            <assign variable="daily" expression="hours*rate"/>
            <assign variable="weekly" expression="daily*7"/>
            <assign variable="monthly" expression="daily*30"/>
            <assign variable="annualy" expression="weekly*52"/>
            <output expression="&quot;Salary in Weekly &quot; &amp; weekly" newline="True"/>
            <output expression="&quot;Salary in Monthlu &quot; &amp; monthly" newline="True"/>
            <output expression="&quot;Salary in Annualy &quot; &amp; annualy" newline="True"/>
        </body>
    </function>
