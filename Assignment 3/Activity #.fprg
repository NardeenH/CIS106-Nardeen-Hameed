            <declare name="ages" type="Integer" array="False" size=""/>
            <declare name="days" type="Integer" array="False" size=""/>
            <declare name="hours" type="Integer" array="False" size=""/>
            <declare name="minutes" type="Integer" array="False" size=""/>
            <assign variable="ages" expression="2022-age"/>
            <assign variable="days" expression="(ages*365)+(month*30)+day"/>
            <assign variable="hours" expression="days*24"/>
            <assign variable="minutes" expression="hours*60"/>
            <output expression="&quot; Days&quot; &amp; days" newline="True"/>
            <output expression="&quot; Hours &quot; &amp; hours" newline="True"/>
            <output expression="&quot; minutes&quot;&amp; minutes" newline="True"/>
